#include<stdint.h>
#include"stm32f4xx.h"

void delay(int T)
{
	int i;
	while(T--)
	{
		for(i=0;i<5000;i++);
	}
}

int main()
{
	int T;
	RCC->AHB1ENR |=7;
	GPIOA->MODER |= 0x10000;
	GPIOB->MODER |= 0x10000;
	GPIOC->MODER |= 0x0;
	while(1)
	{
		if(GPIOC->IDR & 0x200)
		{
		GPIOA->ODR &=~0x100;
		GPIOB->ODR &=~0x100;
	    }
		else{
			GPIOA->ODR |=0x100;
		    GPIOB->ODR |=0x100;
		}
     }
}
